<!DOCTYPE html>
<html lang="en">
  <!-- this is my template -->
  <%- include('partials/head') %>
  <body>
    <%- include('partials/header') %>
    <main>
      <% members.forEach(member => { %>
      <div class="block_member">
        <h2><%= member.name %></h2>
        <ul>
          <li><%= member.id %></li>
          <li><%= member.email %></li>
        </ul>
      </div>
      <% }) %>
    </main>
    <%- include ('partials/footer') %>
  </body>
</html>
